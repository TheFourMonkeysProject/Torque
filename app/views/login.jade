extends login_header
 
block content
  div.container
    form.form-signin(role="form", action='/')
      label Username
      input(type="email", class="form-control",placeholder="Email address", required)
      label Password
      input(type="password",class="form-control",placeholder="Password",required)
      input.btn(class="btn btn-lg btn-primary btn-block" type="submit", value="Log In")
 
  div.footer
